{
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig to read more about this file */
    "target": "es2023",
    "module": "esnext",
    "rootDir": "./src",
    "baseUrl": "./src",
    "paths": {
      "#base/*": [
        "./ld/*"
      ],
      "#ld/*": [
        "./override/*",
        "./ld/*"
      ]
    },
    "outDir": "./dist",
    "removeComments": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}